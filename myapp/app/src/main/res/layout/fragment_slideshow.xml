<?xml version="1.0" encoding="utf-8"?>
<!--suppress AndroidDomInspection -->
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.slideshow.SlideshowFragment">

    <TextView
        android:id="@+id/text_slideshow"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:fontFamily="@font/cairo_extralight"
        android:gravity="center_horizontal"
        android:shadowColor="@color/design_default_color_primary_variant"
        android:text="نصائح للعناية بالطفل"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textColorHighlight="#D7A0E1"
        android:textColorLink="#FFFFFF"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/two"
        android:onClick="onclick12"
        android:layout_width="317dp"
        android:layout_height="57dp"
        android:background="@drawable/ic_group_4"
        android:fontFamily="@font/cairo_extralight"
        android:text="التعامل مع الطفل كثير البكاء"
        android:textColor="#FFFFFF"
        android:textColorHint="#D694E1"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.38"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.297" />

    <Button
        android:id="@+id/three"
        android:layout_width="317dp"
        android:layout_height="57dp"
        android:onClick="onclick13"
        android:background="@drawable/ic_group_4"
        android:fontFamily="@font/cairo_extralight"
        android:text='اللهاية "السكاتة"... فوائدها ومضارها'
        android:textColor="#FFFFFF"
        android:textColorLink="#FDFDFD"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.421"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.443" />

    <Button
        android:id="@+id/one"
        android:layout_width="317dp"
        android:layout_height="57dp"
        android:onClick="onclick11"
        android:background="@drawable/ic_group_4"
        android:fontFamily="@font/cairo_extralight"
        android:text="  افكار لنوم مولودك "
        android:textColor="#FBFAFA"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.429"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.169" />

    <Button
        android:id="@+id/five"
        android:onClick="onclick15"
        android:layout_width="317dp"
        android:layout_height="57dp"
        android:background="@drawable/ic_group_4"
        android:fontFamily="@font/cairo_extralight"
        android:text="أشياء يجب أن تعرفيها عن تدليك مولودك"
        android:textColor="#F8F8F8"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.43"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.709" />

    <Button
        android:id="@+id/four"
        android:layout_width="317dp"
        android:layout_height="57dp"
        android:onClick="onclick14"
        android:background="@drawable/ic_group_4"
        android:fontFamily="@font/cairo_extralight"
        android:text="لهذه الأسباب لا تمسحي لعاب طفلك!"
        android:textColor="#FFFDFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.421"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.571" />

    <Button
        android:id="@+id/six"
        android:layout_width="317dp"
        android:onClick="onclick16"
        android:layout_height="57dp"
        android:fontFamily="@font/cairo_extralight"
        android:text="معلومات عن التسنين يجب معرفتها"
        android:textColor="#FFFAFA"
        android:textSize="18sp"
        android:background="@drawable/ic_group_4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.414"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.854" />

</androidx.constraintlayout.widget.ConstraintLayout>